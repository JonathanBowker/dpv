      {
        "@id": "https://w3id.org/dpv/dpv-skos#NaturalPerson"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "The individual (or category of individuals) whose personal data is being processed"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#note": [
      {
        "@language": "en",
        "@value": "The term 'data subject' is specific to the GDPR, but is functionally equivalent to the term 'individual' and the ISO/IEC term 'PII Principle'."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Data Subject"
      }
    ]
  },
