    <dct:description xml:lang="en">Indicates the status of activity of specified concept</dct:description>
    <rdfs:range rdf:resource="https://w3id.org/dpv/dpv-owl#ActivityStatus"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
  </rdf:Description>
