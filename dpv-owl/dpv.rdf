    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:description xml:lang="en">A high-level Class to describe 'data handling'. This can consist of personal data being processed for a purpose, involving entities, using technical and organisational measures, applicable risks, rights, and legal basis.</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:label xml:lang="en">Personal Data Handling</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Axel Polleres</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Javier Fernández</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#ConsentRevoked">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Julian Flake</dct:creator>
    <rdfs:label xml:lang="en">Consent Revoked</rdfs:label>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-owl#ConsentStatusInvalidForProcessing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">The state where the consent is revoked by an entity other than the data subject and which prevents it from being further used as a valid state</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
    <rdfs:comment xml:lang="en">An example of this state is when a Data Controller stops utilising previously obtaining consent, such as when that service no longer exists</rdfs:comment>
    <dct:source rdf:resource="https://w3id.org/GConsent"/>
  </rdf:Description>
